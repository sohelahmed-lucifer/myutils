
import torch
import torch.nn as nn
class FullyConnectedNet(nn.Module):
    def __init__(self):
        super(FullyConnectedNet, self).__init__()
        self.model = nn.Sequential(
            nn.Linear(64*64, 64),
            nn.ReLU(),
            nn.Linear(64, 256),
            nn.ReLU(),
            nn.Linear(256, 1),
            nn.Sigmoid()
        )
    def forward(self, x):
        x = x.view(x.size(0), -1)  # flatten
        return self.model(x)

net = FullyConnectedNet()
net
